%div
  %strong Customer Name:
  {{ customerName }}
%div
  %strong Customer Number:
  {{ customerNumber }}

%form
  %div
    Customer Name:
    %input{:type => 'text', 'ng-model' => 'cName'}
  %div
    Customer Number:
    %input{:type => 'text', 'ng-model' => 'cNumber'}
  %div
    %input{:type => 'submit', 'ng-click' => 'changeCustomer()'}
